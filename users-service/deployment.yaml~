kind: Deployment
apiVersion: apps/v1
metadata:
  name: users-service
spec:
  replicas: 1
  selector:
    matchLabels:
      app: users-service
  template:
    metadata:
      labels:
        app: users-service
    spec:
      containers:
        -name: users-service
        image: "users-service:v0.1"
        ports:
          - containerPort: "80"
